{
    "name": "imiphp/imi-influxdb",
    "type": "library",
    "license": "MulanPSL-2.0",
    "description": "imi 框架 InfluxDB 组件。imi InfluxDB component.",
    "require": {
        "yurunsoft/influxdb-orm": "^1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~3.4.0",
        "phpstan/phpstan": "~1.8.0",
        "phpunit/phpunit": ">=9",
        "rector/rector": "^0.12.10",
        "imiphp/imi": "2.1.x-dev",
        "imiphp/imi-swoole": "2.1.x-dev",
        "imiphp/imi-workerman": "2.1.x-dev",
        "imiphp/imi-fpm": "2.1.x-dev",
        "imiphp/imi-meter": "2.1.x-dev",
        "swoole/ide-helper": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Imi\\InfluxDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "app\\": "example/",
            "Imi\\InfluxDB\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit -c ./phpunit.xml"
    },
    "suggest": {
        "imiphp/imi-meter": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}